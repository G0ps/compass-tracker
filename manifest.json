{
  "name": "Cookie Access Extension",
  "description": "This extension accesses cookies of a website.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "cookies",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "static/compass.png",
      "48": "static/compass.png",
      "128": "static/compass.png"
    }
  }
}
